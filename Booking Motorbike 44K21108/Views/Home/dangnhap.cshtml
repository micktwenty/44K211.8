                            <input placeholder="Phone" name="Phone" class="phone" type="text" required="">
                            <div class="sign-up">
                                <input type="submit" value="Get New One !" />
                            </div>
                        </form>
                    </div>
                </div>*@
            </div>
        </div>
    </div>
    <div class="footer-w3l">
        <p style=" font-weight:bold;color:black">DESIGN BY 44K21108</p>
    </div>

