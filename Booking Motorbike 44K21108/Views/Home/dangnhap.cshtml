<script src="~/Content/js/dangnhap-1.js"></script>
<script src="~/Content/js/easyResponsiveTabs.js" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#horizontalTab').easyResponsiveTabs({
            type: 'default', //Types: default, vertical, accordion
            width: 'auto', //auto or any width like 600px
            fit: true   // 100% fit in a container
        });
    });
</script>
<body>
    <!---header--->
    <div class="header">
        <h1 style="color:black">Trang Dành Cho Nhà Cung Cấp</h1>
    </div>
    <!---header--->
    <!---main--->
    <div class="main-content-w3">
        <div class="sap_tabs-w3agile">
            <div id="horizontalTab" style="display: block; width: 100%; margin: 0px;">
                <div class="tab">
                    <ul>
                        <li class="resp-tab-item" aria-controls="tab_item-0" role="tab"><span>ĐĂNG KÝ</span></li>
                        @*<li class="resp-tab-item" aria-controls="tab_item-1" role="tab"><span>ĐĂNG NHẬP</span></li>*@
                        @*<li class="resp-tab-item" aria-controls="tab_item-2" role="tab"><span>Reset Password</span></li>*@
                    </ul>
                </div>
                <div class="tab-1 resp-tab-content" aria-labelledby="tab_item-0">
                    <p class="tab-text"></p>
                    <div class="register ">
                        <form action="#" method="post">
                            <input placeholder="Tên Nhà Cung Cấp" name="Name" class="user" type="text" required="">
                            <input placeholder="Tên Tài Khoản" name="Email" class="mess" type="text" required="">
                            <input placeholder="Mật Khẩu" name="Password" class="pass" type="password" required="">
                            <input placeholder="Số Điện Thoại" name="sodienthoai" class="sodienthoai" type="text" required="">
                            <input placeholder="Địa chỉ" name="diachi" class="diachi" type="text" required="">


                            <div class="sign-up">
                                <input type="submit" value="Đăng Ký">
                            </div>
                        </form>
                    </div>
                </div>
                @*<div class="tab-2 resp-tab-content" aria-labelledby="tab_item-1">
                    
                    <h4 class="text-center"></h4>
                    <div class="register">
                        <form action="#" method="post">
                            <input placeholder="Tên Tài Khoản" name="Email" class="user" type="text" required="">
                            <input placeholder="Mật Khẩu" name="Password" class="pass" type="password" required="">
                            
                            <div class="sign-up">
                                <input type="submit" value="Đăng Nhập" />
                            </div>
                        </form>
                        <h3><a href="#"></a></h3>
                    </div>
                </div>
                <div class="tab-3 resp-tab-content" aria-labelledby="tab_item-2">
                    <p class="tab-text">Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Lorem ipsum dolor. reprehenderit</p>

                    <div class="register">
                        <form action="#" method="post">
                            <input placeholder="Email" name="Email" class="user" type="text" required="">
